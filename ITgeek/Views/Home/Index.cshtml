@model IEnumerable<ITgeek.uzytkownik>

<div class="row">
    <div class="col-md-4">
        <h2>@ViewBag.Title</h2>
        <h4>@ViewBag.Message</h4>
    </div>


    <div class="col-md-4">
        <h3>Lista kont:</h3>

        <table class="table">
            <tr>
                <th>
                    Imię
                </th>
                <th>
                    Nazwisko
                </th>
                <th>
                    Nazwa
                </th>
                <th>
                   E-mail

                </th>
                <th>
                   Hasło
                </th>
                
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.imie)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.nazwisko)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.wyswietlana_nazwa)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.haslo)
                    </td>
                    <!--
                   <td>
                     @Html.ActionLink("Edit", "Edit", new { id = item.id_uzytkownik }) |
                     @Html.ActionLink("Details", "Details", new { id = item.id_uzytkownik }) |
                     @Html.ActionLink("Delete", "Delete", new { id = item.id_uzytkownik })
                 </td>


                -->
                </tr>
            }

        </table>


    </div>







</div>